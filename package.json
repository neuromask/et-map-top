{
  "name": "vue-bootstrap-boilerplate",
  "version": "2.1.0",
  "private": false,
  "description": "Vue-Cli-3, Vuex, Vue-Router, Bootstrap 4, Babel, ESLint, Axios, SCSS and Pug/HTML boilerplate.",
  "author": "Leo & Aleksandr",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.1",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.17.2",
    "mini-css-extract-plugin": "^1.6.0",
    "scriptjs": "^2.5.9",
    "vue": "^2.6.14",
    "vue-gtag": "^1.16.1",
    "vue-mobile-detection": "^1.0.0",
    "vue-plugin-load-script": "^1.3.2",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "~3.2.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "1.2.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-airbnb": "^5.0.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.11.1",
    "node-sass": "^6.0.1",
    "pug-plain-loader": "^1.1.0",
    "sass": "~1.32.12",
    "sass-loader": "7.3.1",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "node": false
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  },
  "nohoist": [
    "**/@vue/cli-plugin-eslint",
    "**/@vue/cli-plugin-eslint/**"
  ],
  "contributors": [
    "kaangokdemir",
    "malinsdd"
  ]
}
